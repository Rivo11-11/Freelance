{
  "version": 2,
  "builds": [
    {
      "src": "src/app.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/swagger.json",
      "dest": "src/app.ts"
    },
    {
      "src": "/api-docs",
      "dest": "src/app.ts"
    },
    {
      "src": "/docs",
      "dest": "src/app.ts"
    },
    {
      "src": "/(.*)",
      "dest": "src/app.ts"
    }
  ],
  "functions": {
    "src/app.ts": {
      "maxDuration": 30
    }
  }
} 