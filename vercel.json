{
  "version": 2,
  "builds": [
    {
      "src": "src/app.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api-docs/(.*)",
      "dest": "src/app.ts"
    },
    {
      "src": "/(.*)",
      "dest": "src/app.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "PRODUCTION_SERVER_URL": "https://luby-rafiks-projects-827f7443.vercel.app"
  }
} 